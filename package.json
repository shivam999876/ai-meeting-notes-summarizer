{
  "name": "ai-meeting-notes-summarizer",
  "version": "1.0.0",
  "description": "AI-powered meeting transcript summarizer with email sharing capabilities",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
